print(5.f)
print(5.4f)
number = 5
print(numbr.f)
print(numbere.f)
print(number.f)
print(number.4f)
print(f"{number.4f}")
print(f"{number}")
print(f"{number.4f}")
print(f"{int(number).4f}")
print(f"{float(number).4f}")
number = 0.5
print(f"{float(number).4f}")
print(f"{number.4f}")
number = 5
print(f"{number:.4f}")
